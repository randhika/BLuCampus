        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/guest"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="-30dp"
            android:elevation="70dp"/>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:elevation="30dp"
            app:cardPreventCornerOverlap="false"
            app:cardCornerRadius="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:textAlignment="center"
                    style="@style/AlertDialog.AppCompat"
                    android:textColor="#fff"
                    android:background="#FF0000"
                    android:padding="10dp"
                    android:textSize="14dp"
                    android:visibility="gone"
                    android:id="@+id/result"
                    android:layout_marginBottom="10dp"/>


                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:textColorHint="@color/colorPrimary">
                    <EditText
                        android:id="@+id/txtUsername"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Username"
                        android:labelFor="@+id/txtUsername"
                        android:drawableStart="@drawable/ic_account_circle_24dp1"
                        android:drawableLeft="@drawable/ic_account_circle_24dp1"
                        android:singleLine="true"
                        style="@style/EditTextThemeR"
                        />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColorHint="@color/colorPrimary">
                    <EditText
                        android:id="@+id/txtName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Nama"
                        android:inputType="text"
                        android:labelFor="@+id/txtName"
                        android:drawableStart="@drawable/ic_person_24dp_black"
                        android:drawableLeft="@drawable/ic_person_24dp_black"
                        style="@style/EditTextThemeR"
                        />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColorHint="@color/colorPrimary">
                    <EditText
                        android:id="@+id/txtTelp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Telp/Hp"
                        android:inputType="phone"
                        android:labelFor="@+id/txtTelp"
                        android:drawableStart="@drawable/ic_phone_iphone_24dp"
                        android:drawableLeft="@drawable/ic_phone_iphone_24dp"
                        style="@style/EditTextThemeR"
                        />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColorHint="@color/colorPrimary">
                    <EditText
                        android:id="@+id/txtEmail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Email"
                        android:inputType="textEmailAddress"
                        android:labelFor="@+id/txtEmail"
                        android:drawableStart="@drawable/ic_email_24dp_black"
                        android:drawableLeft="@drawable/ic_email_24dp_black"
                        style="@style/EditTextThemeR"
                        />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColorHint="@color/colorPrimary"
                    app:passwordToggleEnabled="true">

                    <EditText
                        android:id="@+id/txtPassword"
                        style="@style/EditTextThemeR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:drawableLeft="@drawable/ic_https_24dp_black"
                        android:drawableStart="@drawable/ic_https_24dp_black"
                        android:hint="Password"
                        android:inputType="textPassword"
                        android:labelFor="@+id/txtPassword" />
                </android.support.design.widget.TextInputLayout>

                <Button
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:padding="10dp"
                    android:paddingLeft="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/button"
                    android:text="Daftar"
                    android:drawableLeft="@drawable/ic_check_24dp"
                    android:pointerIcon="hand"
                    android:textColor="#fff"
                    android:typeface="serif"
                    android:onClick="btnLogin"
                    android:id="@+id/btnDaftar"
                    android:nestedScrollingEnabled="false"
                    android:layout_marginBottom="30dp"/>
            </LinearLayout>
        </android.support.v7.widget.CardView>


